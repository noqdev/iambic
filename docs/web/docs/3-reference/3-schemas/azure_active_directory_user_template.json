    "IambicManaged": {
      "title": "IambicManaged",
      "description": "An enumeration.",
      "enum": [
        "undefined",
        "read_and_write",
        "import_only",
        "enforced",
        "disabled"
      ]
    },
    "UserStatus": {
      "title": "UserStatus",
      "description": "An enumeration.",
      "enum": [
        "active",
        "provisioned",
        "deprovisioned"
