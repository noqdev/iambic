    "version": "0.2.0",
    "configurations": [
        {
            "name": "Iambic Plan",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "plan",
            ],
            "justMyCode": false
        },
        {
            "name": "Iambic Apply All Templates",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "apply",
            ],
            "justMyCode": true
        },
        {
            "name": "Iambic - Apply Specific Template",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "args": [
                "apply",

                "--template",
