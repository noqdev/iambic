    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Iambic: Functional Tests",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "console": "integratedTerminal",
            "args": [
                "--disable-warnings",
                "--color=yes",
                "--verbose",
                // Temporarily uncomment locally to limit scope of tests
                // "-k",
                // "test_google",
                "${workspaceFolder}/functional_tests",
            ],
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false
        },
        {
            "name": "Iambic: Unit Tests",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "--ignore",
                "functional_tests/",
                // Temporarily uncomment locally to limit scope of tests
                // "-k",
                // "test_main",
                "${workspaceFolder}/",
            ],
            "justMyCode": false
        },
        {
            "name": "Iambic: Plan",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "plan",
            ],
            "justMyCode": false
        },
        {
            "name": "Iambic: Apply Specific Template",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "args": [
                "apply",
            ],
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": true
        },
        {
            "name": "Iambic: Apply IAMbic Enforced-Mode Templates",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "args": [
                "apply",
                "--enforced-only"
            ],
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": true
        },
        {
            "name": "Iambic: Git-Apply",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "git-apply",
            ],
            "justMyCode": true
        },
        {
            "name": "Iambic: Git-Apply from sha to sha",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "git-apply",

                "--from-sha",
                "${env:IAMBIC_FROM_SHA}",
                "--to-sha",
                "${env:IAMBIC_TO_SHA}",
            ],
            "justMyCode": true
        },
        {
            "name": "Iambic: Git-Plan",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "args": [
                "plan",
                "--git-aware",
            ],
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": true
        },
        {
            "name": "Iambic: Import",
            "type": "python",
            "request": "launch",
            "module": "iambic.main",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env",
